{
  "name": "tuebi-api",
  "version": "1.0.0",
  "description": "A minimal bookmark manager",
  "author": "Nguyen Cong Cuong",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && cp -R package.json dist/package.json && cp -R node_modules dist/node_modules",
    "start": "node main.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.2",
    "@azure/storage-blob": "^12.12.0",
    "@azure/storage-queue": "^12.11.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/throttler": "^3.1.0",
    "@types/lodash": "^4.14.185",
    "axios": "^1.2.0",
    "crypto-js": "^4.1.1",
    "cryptr": "^6.0.3",
    "helmet": "^6.0.0",
    "htmlparser2": "^8.0.1",
    "jose": "^4.11.4",
    "lodash": "^4.17.21",
    "mjml": "^4.13.0",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.31.3",
    "superagent": "8.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/mjml": "^4.7.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-auth-token": "^1.0.1",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "jest": "28.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
